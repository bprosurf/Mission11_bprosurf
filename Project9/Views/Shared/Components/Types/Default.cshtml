
@{
}

@model IEnumerable<string>


<div class="mt-1">
    <a asp-controller="Home" asp-action="Index" asp-route-Category="" asp-route-pageNum="1" class="btn btn-outline-secondary mt-1 btn-block">Home</a>


    @foreach (string t in Model)
        {
        <div class="mt-1" >
            <a asp-controller="Home" asp-action="Index" asp-route-Category="@t" asp-route-pageNum="1" class="btn @(ViewBag.SelectedType == t ? "btn-danger" :  "btn-outline-secondary") btn-block">@t</a>
        </div>
        }
</div>